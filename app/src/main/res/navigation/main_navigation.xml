            app:argType="integer"
            android:defaultValue="0"/>
        <argument
            android:name="delivery_type"
            app:argType="integer"
            android:defaultValue="0"/>
        <argument
            android:name="comment"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="null" />
