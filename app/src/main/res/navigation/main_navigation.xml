            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
            android:name="request_or_parcel"
            app:argType="integer"
            android:defaultValue="-1"/>
        <argument
            android:name="sender_country_id"
            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
            android:name="sender_region_id"
            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
