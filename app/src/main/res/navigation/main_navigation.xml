        <action
            android:id="@+id/action_myBidsFragment_to_partyFragment"
            app:destination="@id/partyFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_open_enter"
            app:popExitAnim="@anim/fragment_open_exit">
        </action>
        <action
            android:id="@+id/action_myBidsFragment_to_calculatorFragment"
            app:destination="@id/calculatorFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_open_enter"
            app:popExitAnim="@anim/fragment_open_exit"/>
        <action
            android:id="@+id/action_myBidsFragment_to_parcelDataFragment"
            app:destination="@id/invoiceDataFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_open_enter"
            app:popExitAnim="@anim/fragment_open_exit">
        </action>
        <action
            android:id="@+id/action_myBidsFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:enterAnim="@anim/fragment_open_enter"
            app:exitAnim="@anim/fragment_open_exit"
            app:popEnterAnim="@anim/fragment_open_enter"
            app:popExitAnim="@anim/fragment_open_exit"/>
<!--        <argument-->
<!--            android:name="party_id"-->
<!--            app:argType="long"-->
<!--            android:defaultValue="-1L"/>-->
        <argument
            android:name="courier_id"
            app:argType="long"
            android:defaultValue="-1L"/>
<!--        <argument-->
<!--            android:name="parcel_quantity"-->
<!--            app:argType="integer"-->
<!--            android:defaultValue="-1"/>-->
<!--        <argument-->
<!--            android:name="parcel_id"-->
<!--            app:argType="long"-->
<!--            android:defaultValue="-1L"/>-->
    </fragment>
    <fragment
