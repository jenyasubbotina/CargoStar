            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
            android:name="qr_code"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="tracking_code"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="transportation_status_name"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="transportation_status_id"
            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
            android:name="city_from"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="city_to"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="courier_id"
            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
            android:name="payment_status_id"
            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
            android:name="party_qr_code"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="instructions"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="direction"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="arrival_date"
            app:argType="string"
            app:nullable="true"/>
        <argument
            android:name="provider_id"
            app:argType="long"
            android:defaultValue="-1L"/>
        <argument
            android:name="request_id"
            app:argType="long"
            android:defaultValue="-1L"/>
